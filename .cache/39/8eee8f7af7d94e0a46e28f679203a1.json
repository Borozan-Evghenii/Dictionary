margin: 0 auto;\r\n}\r\n\r\n.header-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.functional {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n\r\n/*Font Switcher*/\r\n\r\n.dropdown {\r\n  display: flex;\r\n  gap: 10px;\r\n  padding: 10px 10px;\r\n  font-family: inherit;\r\n  font-weight: inherit;\r\n  font-size: inherit;\r\n  color: var(--primar-color);\r\n  background-color: transparent;\r\n  cursor: pointer;\r\n}\r\n\r\n.font-dropdown {\r\n  padding: 0 20px;\r\n  border: 1px solid transparent;\r\n  border-bottom: none;\r\n  border-right: 1px solid var(--primar-color);\r\n  position: relative;\r\n  cursor: pointer;\r\n}\r\n.font-dropdown:hover {\r\n  border-color: var(--gray);\r\n  border-radius: 20px 20px 0 0;\r\n}\r\n.font-dropdown:hover .dropdown-content {\r\n  height: auto;\r\n  opacity: 1;\r\n  transition: all 0.4s ease;\r\n}\r\n\r\n.dropdown-content {\r\n  position: absolute;\r\n  top: 40px;\r\n  right: 0;\r\n  left: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: left;\r\n  font-weight: var(--font-weight);\r\n  font-size: 14px;\r\n  background-color: var(--white);\r\n  border-radius: 0 0 20px 20px;\r\n  border: 1px solid var(--gray);\r\n  border-top: none;\r\n  overflow: hidden;\r\n  height: 0%;\r\n  transition: all 0.4s ease;\r\n  opacity: 0;\r\n}\r\n\r\n.option {\r\n  padding: 15px 20px;\r\n  background-color: var(--bg);\r\n  color: var(--primar-color);\r\n}\r\n.dropdown-content li:hover {\r\n  cursor: pointer;\r\n  background-color: var(--gray);\r\n  color: var(--dark);\r\n}\r\n\r\n.dropdown svg {\r\n  width: 10px;\r\n}\r\n\r\n/*Theme Toggler*/\r\n\r\n.toggler {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n\r\n.theme-toggle {\r\n  width: 35px;\r\n  height: 20px;\r\n  background-color: transparent;\r\n  border-radius: 100px;\r\n  border: 1px solid var(--primar-color);\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: flex-start;\r\n  padding: 3px;\r\n  transition: all 0.4s ease-in-out;\r\n  cursor: pointer;\r\n}\r\n\r\n.theme-toggle .indicator {\r\n  background-color: var(--primar-color);\r\n  width: 12px;\r\n  height: 14px;\r\n  border-radius: 100px;\r\n  transition: all 0.4s ease-in-out;\r\n}\r\n.dark {\r\n  align-items: flex-end;\r\n  transition: all 0.4s ease-in-out;\r\n}\r\n\r\n/*Search Form*/\r\nform {\r\n  margin-top: 50px;\r\n  display: flex;\r\n  background-color: var(--gray);\r\n  align-items: center;\r\n  padding: 0 24px;\r\n  border-radius: 20px;\r\n}\r\n\r\n.search {\r\n  width: 100%;\r\n  background-color: var(--gray);\r\n  outline: none;\r\n  padding: 24px 0;\r\n  font-size: inherit;\r\n  font-family: inherit;\r\n  font-weight: inherit;\r\n}\r\n\r\n.search-button {\r\n  cursor: pointer;\r\n}\r\n\r\n/*Result Block*/\r\n\r\n.phonetica {\r\n  padding: 50px 0 0 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  &-text {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 10px;\r\n    color: var(--primar-color);\r\n    &__word {\r\n      font-size: 55px;\r\n    }\r\n    &__subtitle {\r\n      color: var(--second-color);\r\n      font-size: 24px;\r\n    }\r\n  }\r\n}\r\n\r\n.audio {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 75px;\r\n  height: 75px;\r\n  background-color: var(--second-color-light);\r\n  border-radius: 100px;\r\n  cursor: pointer;\r\n  border: 1px solid transparent;\r\n  &:hover {\r\n    border-color: var(--second-color);\r\n  }\r\n  svg {\r\n    padding-left: 3px;\r\n  }\r\n}\r\n\r\n.meaning {\r\n  padding: 50px 0;\r\n  color: var(--primar-color);\r\n  &-header {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    &__text {\r\n      font-size: 18px;\r\n      font-style: italic;\r\n      line-height: 1;\r\n    }\r\n  }\r\n  &-content {\r\n    padding-top: 50px;\r\n  }\r\n\r\n  .synonym {\r\n    padding: 7px 15px;\r\n    line-height: 1;\r\n    border-radius: 100px;\r\n    background-color: var(--second-color-light);\r\n  }\r\n}\r\n\r\n.line {\r\n  width: 100%;\r\n  height: 1px;\r\n  background-color: var(--primar-color);\r\n  opacity: 0.1;\r\n}\r\n\r\n[class^=\"meaning-list-\"] {\r\n  margin: 20px 0 40px 0;\r\n  list-style: circle;\r\n  padding-left: 40px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n}\r\n[class^=\"meaning-synonymes-\"] {\r\n  display: flex;\r\n  align-items: center;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  white-space: nowrap;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f1d6974a9b5ea2561eb10cfd6d236f3e","cacheData":{"env":{}}}